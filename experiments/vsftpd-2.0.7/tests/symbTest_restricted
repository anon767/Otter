STATSFILE=../vsftpd.stats

TOPDIR=../../..
LIBCDIR=$TOPDIR/libc
MOCKEDDIR=$TOPDIR/mockedFns

TEST_NAME=$1
shift
MACRO='-DTUNABLE_SSL_ENABLE=0 -DTUNABLE_LOCAL_ENABLE=0 -DTUNABLE_ANONYMOUS_ENABLE=1'

echo \#BEGIN TEST_SYMBOLIC TEST_NAME=$TEST_NAME MACRO=\"$MACRO\"
date

CILLY_DONT_COMPILE_AFTER_MERGE= \
$TOPDIR/cil/bin/cilly --merge --useLogicalOperators \
-D_FILE_OFFSET_BITS=64 \
-I $MOCKEDDIR \
../vsftpd_comb.c --covStats=$STATSFILE --doexecute $TEST_NAME.c --printLittle --lineCov --timeout=14400 \
$MACRO \
--marshalCoverageTo=/fs/skoll/symexe/data/vsftpd-2.0.7/marshalledData/${TEST_NAME}_restricted.data

echo \#DONE TEST_SYMBOLIC TEST_NAME=$TEST_NAME MACRO=\"$MACRO\"
date
