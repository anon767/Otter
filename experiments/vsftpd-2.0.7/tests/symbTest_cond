STATSFILE=../vsftpd.stats

TOPDIR=../../..
LIBCDIR=$TOPDIR/libc
MOCKEDDIR=$TOPDIR/mockedFns

TEST_NAME=$1
shift

MACRO=--condCov

echo \#BEGIN TEST_SYMBOLIC TEST_NAME=$TEST_NAME MACRO=\"$MACRO\"
date

CILLY_DONT_COMPILE_AFTER_MERGE= \
$TOPDIR/cil/bin/cilly --merge --useLogicalOperators \
-D_FILE_OFFSET_BITS=64 \
-I $MOCKEDDIR \
../vsftpd_comb_unfolded.c $TEST_NAME.c common_init.c \
--lineCov --edgeCov $MACRO --covStats=$STATSFILE --doexecute --printLittle --timeout=14400 \
--marshalCoverageTo=/fs/skoll/symexe/data/vsftpd-2.0.7/marshalledData/${TEST_NAME}_cond.data

echo \#DONE TEST_SYMBOLIC TEST_NAME=$TEST_NAME MACRO=\"$MACRO\"
date
