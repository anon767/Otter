#
#  $1 = DATA_DIR
#  $2 = PROJ_DIR
#  $3 = test name
#  $4 = numVars
#
DATA_DIR=$1
PROJ_DIR=$2
TEST_NAME=$3
NUMVARS=$4

TOPDIR=../../..
MARSHALDIR=$DATA_DIR/marshal
POSSIBLE=$DATA_DIR/ngircdPossibleValues


echo \#BEGIN TEST_VARDEPS TEST_NAME=$TEST_NAME NUMVARS=$NUMVARS
date


$TOPDIR/cil/src/se/postprocessing/guaranteedCoverage \
	$MARSHALDIR/$TEST_NAME.marshal \
	--fileWithPossibleValues $POSSIBLE 

echo \#DONE TEST_VARDEPS TEST_NAME=$TEST_NAME NUMVARS=$NUMVARS
date
